{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Anuj5970-DataFactory-UAT"
		},
		"Ls_outputFolder_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Ls_outputFolder'"
		},
		"ls_BlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_BlobStorage'"
		},
		"lS_HttpServer_Dummy_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": " https://www.learningcontainer.com/wp-content/uploads/2020/05/sample-zip-file.zip"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Anuj5970-DataFactory-UAT_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Ls_outputFolder_connectionString": {
						"value": "[parameters('Ls_outputFolder_connectionString')]"
					},
					"ls_BlobStorage_connectionString": {
						"value": "[parameters('ls_BlobStorage_connectionString')]"
					},
					"lS_HttpServer_Dummy_properties_typeProperties_url": {
						"value": "[parameters('lS_HttpServer_Dummy_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}